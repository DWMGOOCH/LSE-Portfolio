   "source": [
    "# Determine polarity of review columns. \n",
    "reviews_pol['Word_pol'] = reviews_pol['Word'].apply(generate_polarity)\n",
    "\n",
    "\n",
    "# View output.\n",
    "reviews_pol.sort_values(by='Word_pol', ascending=False)"
