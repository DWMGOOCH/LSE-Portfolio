    "# Apply the final model.\n",
    "# Use five clusters.\n",
    "kmeans = KMeans(n_clusters = 5, \n",
    "                max_iter = 15000,\n",
    "                init='k-means++',\n",
    "                random_state=42).fit(x)\n",
    "\n",
    "clusters = kmeans.labels_\n",
