    "# Top 20 negative reviews.\n",
    "negative_sentiment_review = df_clean.nsmallest(20, 'polarity_review')\n",
    "\n",
    "# Drop uneccessary columns\n",
    "negative_sentiment_review = negative_sentiment_review[['review', 'polarity_review']]\n",
    "\n",
    "# Adjust the column width.\n",
    "negative_sentiment_review.style.set_properties(subset=['review'], **{'width': '1800px'})"
   ]
